            android:name="com.ensiie.iaato.MainActivity"
            android:label="@string/app_name"
            android:singleUser="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ensiie.iaato.ActiviteTab"
            android:finishOnTaskLaunch="true" >
        </activity>
        <activity android:name="com.ensiie.iaato.SwipeActivity" >
        </activity>
        <activity android:name="com.ensiie.iaato.SiteActivity" >
        </activity>
        <activity android:name="com.ensiie.iaato.ShipActivity" >
        </activity>
        <activity android:name="com.ensiie.iaato.InfoActivity" >
        </activity>
        <activity android:name="com.ensiie.iaato.StepActivity" >
        </activity>
        <activity
            android:name="com.ensiie.iaato.ModifActivity"
            android:label="@string/title_activity_modif" >
        </activity>
